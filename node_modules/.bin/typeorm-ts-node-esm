#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/linxnext/Documents/revise-wise/revisewise-server/node_modules/.pnpm/typeorm@0.3.22_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3_/node_modules/typeorm/node_modules:/home/linxnext/Documents/revise-wise/revisewise-server/node_modules/.pnpm/typeorm@0.3.22_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3_/node_modules:/home/linxnext/Documents/revise-wise/revisewise-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/linxnext/Documents/revise-wise/revisewise-server/node_modules/.pnpm/typeorm@0.3.22_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3_/node_modules/typeorm/node_modules:/home/linxnext/Documents/revise-wise/revisewise-server/node_modules/.pnpm/typeorm@0.3.22_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3_/node_modules:/home/linxnext/Documents/revise-wise/revisewise-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
